#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    char c;
    char s[50];
    char sen[100];
    scanf("%c", &c);
    scanf("%s", s);
    scanf(" %[^\n]%*c", sen);
    printf("%c\n%s\n%s",c,s,sen);
    return 0;
}

